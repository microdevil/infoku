<div class="container">
        <div class="row centered-form">
        <div class="col-xs-12 col-sm-8 col-md-4 col-sm-offset-2 col-md-offset-4">
          <div class="panel panel-default">
            <div class="panel-heading">
              <h3 class="panel-title">Please sign up <small>It's free!</small></h3>
            </div>
            <div class="panel-body">
              <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), role:"form") do |f| %>
              <%= devise_error_messages! %>
                <div class="form-group">
                  <%= f.text_field :name, class: "form-control input-sm", placeholder:"Name" %>
                </div>
                <div class="form-group">
                  <%= f.text_field :phone, :autofocus => true, class: "form-control input-sm", placeholder: "Phone" %>
                </div>
                <div class="form-group">
                  <%= f.text_area :address, :autofocus => true, class: "form-control input-sm", placeholder: "Address" %>
                </div>
                <div class="form-group">
                  <%= f.text_field :current_job, :autofocus => true, class: "form-control input-sm", placeholder: "Current Job" %>
                </div>
                <div class="form-group">
                  <%= f.email_field :email, :autofocus => true, class: "form-control input-sm", placeholder: "Email Address" %>
                </div>
                <div class="row">
                  <div class="col-xs-6 col-sm-6 col-md-6">
                    <div class="form-group">
                      <%= f.password_field :password, class: "form-control input-sm", placeholder: "Password"%>
                    </div>
                  </div>
                  <div class="col-xs-6 col-sm-6 col-md-6">
                    <div class="form-group">
                      <%= f.password_field :password_confirmation, class: "form-control input-sm", placeholder: "Confirm Password" %>
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <%= f.text_field :security_code, :autofocus => true, class: "form-control input-sm", placeholder: "Securty Code" %>
                </div>
                <%= f.submit "Sign up", class: "btn btn-info btn-block" %>
                <%= render "users/shared/links" %>
              
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
<script type="text/javascript">
 $(".datepicker").datepicker();
</script>
